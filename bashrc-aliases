source .config/luke-config/general-config

## General Movement
#------------------------------------------------------------
alias asd="cd"
alias d="cd $HOME/Desktop"
alias dv="cd $HOME/Dev"


## Pacman/Yay
#------------------------------------------------------------
alias pps="pacman -Ss"
alias sps="sudo pacman -S"
alias ys="yay -Ss"


## C/C++
#------------------------------------------------------------
alias m="make"
alias mc="make clean"
alias a="./bin/app"


## Spotify
#------------------------------------------------------------
alias sp="spotifyd --config-path ~/.config/spotifyd/spotifyd.conf --no-daemon"

